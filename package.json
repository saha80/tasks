    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.3"
  },
  "engines": {
    "node": ">=18.15.0"
